{
    "name": "php-etl/json-plugin",
    "description": "Plugin for JSON Extractor and Loader",
    "type": "gyroscops-plugin",
    "require": {
        "php": "^8.2",
        "php-etl/configurator-contracts": "^0.6@dev",
        "nikic/php-parser": "^4.10",
        "symfony/config": "^5.2 || ^6.0",
        "symfony/expression-language": "^5.2 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.26.18",
        "rector/rector": "^0.15",
        "php-etl/phpunit-extension": "^0.5@dev",
        "adlawson/vfs": "dev-develop"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Kiboko\\Plugin\\JSON\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Plugin\\JSON\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.3.x-dev",
            "dev-feature/qualityflow-improvments": "0.3.x-dev"
        },
        "gyroscops": {
            "plugins": [
                "Kiboko\\Plugin\\JSON\\Service"
            ]
        }
    }
}
