{
    "name": "php-etl/json-plugin",
    "description": "Plugin for JSON Extractor and Loader",
    "type": "gyroscops-plugin",
    "require": {
        "php": "^8.1",
        "php-etl/configurator-contracts": "^0.4.0",
        "nikic/php-parser": "^4.10",
        "symfony/config": "^5.2",
        "symfony/expression-language": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "php-etl/phpunit-extension": "^0.4.0",
        "adlawson/vfs": "dev-develop"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Kiboko\\Plugin\\JSON\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Plugin\\JSON\\": "tests/functional/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        },
        "gyroscops": {
            "plugins": [
                "Kiboko\\Plugin\\JSON\\Service"
            ]
        }
    }
}
